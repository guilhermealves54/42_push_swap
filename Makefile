clean:
	rm -f $(OBJS)

fclean: clean
	rm -f $(NAME)
		@if [ -f checker_linux ]; then \
		rm -f checker_linux; \
	fi
	@if [ -d push_swap_visualizer ]; then \
		rm -r -f push_swap_visualizer; \
	fi

re: fclean all
